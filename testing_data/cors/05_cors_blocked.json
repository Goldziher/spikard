{
  "name": "CORS request blocked",
  "description": "Tests CORS request from disallowed origin",
  "source": {
    "framework": "starlette",
    "test_file": "tests/test_cors.py",
    "test_function": "test_cors_blocked"
  },
  "handler": {
    "route": "/items/",
    "method": "GET",
    "cors": {
      "allowed_origins": [
        "https://example.com"
      ],
      "allowed_methods": [
        "GET",
        "POST"
      ],
      "allowed_headers": [
        "Content-Type"
      ]
    },
    "parameters": {
      "headers": {
        "Origin": {
          "type": "string",
          "optional": true
        }
      }
    }
  },
  "request": {
    "method": "GET",
    "path": "/items/",
    "headers": {
      "Origin": "https://malicious-site.com"
    }
  },
  "expected_response": {
    "status_code": 403,
    "body": {
      "detail": "CORS request from origin 'https://malicious-site.com' not allowed"
    }
  },
  "notes": "Server can block requests from disallowed origins"
}
