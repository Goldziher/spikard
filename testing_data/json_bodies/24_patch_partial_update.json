{
  "name": "PATCH partial update",
  "description": "Tests PATCH request with partial object update",
  "source": {
    "framework": "fastapi",
    "test_file": "tests/test_tutorial/test_body_updates/test_tutorial001.py",
    "test_function": "test_patch_partial_update"
  },
  "handler": {
    "route": "/items/{id}",
    "method": "PATCH",
    "body_schema": {
      "type": "object",
      "properties": {
        "price": {
          "type": "number"
        }
      },
      "required": [
        "price"
      ]
    },
    "parameters": {
      "path": {
        "id": {
          "type": "string"
        }
      }
    }
  },
  "request": {
    "method": "PATCH",
    "path": "/items/1",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "price": 45.0
    }
  },
  "expected_response": {
    "status_code": 200,
    "body": {
      "name": "Original Item",
      "price": 45.0,
      "description": "Original description"
    }
  },
  "notes": "PATCH updates only 'price' field, other fields unchanged"
}
