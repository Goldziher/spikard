{
  "name": "Datetime field - success",
  "description": "Tests datetime field with ISO datetime format",
  "source": {
    "framework": "fastapi",
    "test_file": "tests/test_tutorial/test_body/test_tutorial001.py",
    "test_function": "test_datetime_field"
  },
  "handler": {
    "route": "/events/",
    "method": "POST",
    "body_schema": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        }
      },
      "additionalProperties": false,
      "required": [
        "name",
        "created_at"
      ]
    }
  },
  "request": {
    "method": "POST",
    "path": "/events/",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "name": "Meeting",
      "created_at": "2024-03-15T10:30:00Z"
    }
  },
  "expected_response": {
    "status_code": 200,
    "body": {
      "name": "Meeting",
      "created_at": "2024-03-15T10:30:00Z"
    }
  },
  "notes": "Datetime field in ISO 8601 format with UTC timezone"
}
