				}
			},
			"additionalProperties": false,
			"required": ["name", "category"]
		}
	},
	"request": {
		"method": "POST",
		"path": "/items/",
		"headers": {
			"Content-Type": "application/json"
		},
		"body": {
			"name": "Item",
			"category": "electronics"
		}
	},
	"expected_response": {
		"status_code": 200,
		"body": {
			"name": "Item",
			"category": "electronics"
		}
