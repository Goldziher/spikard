{
	"name": "Enum field - success",
	"description": "Tests enum field with valid enum value",
	"source": {
		"framework": "fastapi",
		"test_file": "tests/test_tutorial/test_body/test_tutorial001.py",
		"test_function": "test_enum_field"
	},
	"handler": {
		"route": "/items/",
		"method": "POST",
		"body_schema": {
			"type": "object",
