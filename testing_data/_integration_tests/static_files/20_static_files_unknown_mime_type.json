{
	"name": "Unknown MIME type fallback",
	"description": "Tests fallback to application/octet-stream for files with unknown extensions",
	"category": "static_files",
	"handler": {
		"route": "/*filepath",
		"method": "GET",
		"middleware": {
			"static_files": {
				"root_dir": "./public",
				"cache_control": "public, max-age=3600"
			}
		}
	},
	"request": {
		"method": "GET",
		"path": "/files/data.xyz"
	},
	"expected_response": {
		"status_code": 200,
		"headers": {
			"Content-Type": "application/octet-stream",
			"Cache-Control": "public, max-age=3600",
			"ETag": "\"nop012\"",
			"Accept-Ranges": "bytes",
			"Content-Disposition": "attachment; filename=\"data.xyz\""
		},
		"body": {
			"simulated_file": "files/data.xyz",
			"simulated_size_bytes": 4096
		}
	},
	"tags": ["static_files", "middleware", "mime_types", "unknown_extension", "octet_stream"],
	"notes": "Files with unknown extensions default to application/octet-stream per RFC 2046. Content-Disposition: attachment triggers download"
}
