{
	"name": "304 Not Modified - Cached content valid",
	"description": "Tests 304 Not Modified for cached resources",
	"source": {
		"framework": "starlette",
		"test_file": "tests/test_responses.py",
		"test_function": "test_not_modified"
	},
	"handler": {
		"route": "/status-test/{code}",
		"method": "GET",
		"parameters": {
			"path": {
				"code": {
					"type": "string"
				}
			},
			"headers": {
				"If-None-Match": {
					"type": "string",
					"optional": true
				}
			}
		}
	},
	"request": {
		"method": "GET",
		"path": "/status-test/304",
		"headers": {
			"If-None-Match": "\"abc123\""
		}
	},
	"expected_response": {
		"status_code": 304
	},
	"notes": "304 Not Modified - cached content still valid (ETag match)"
}
