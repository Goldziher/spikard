{
	"name": "17_extremely_long_string",
	"description": "Very long string values should be validated against maxLength",
	"category": "edge_cases",
	"source": {
		"framework": "spikard",
		"inspired_by": "litestar",
		"test_case": "Long string boundary testing"
	},
	"handler": {
		"route": "/text",
		"method": "POST",
		"body_schema": {
			"type": "object",
			"required": ["content"],
			"properties": {
				"content": {
					"type": "string",
					"maxLength": 10000
				}
			}
		}
	},
	"request": {
		"method": "POST",
		"path": "/text",
		"body": {
			"content": ""
		}
	},
	"expected_response": {
		"status_code": 422,
		"body": {
			"type": "https://spikard.dev/errors/validation-error",
			"title": "Request Validation Failed",
			"status": 422,
			"detail": "1 validation error in request",
			"errors": [
				{
					"type": "validation_error",
					"loc": ["body", "content"],
					"msg": "String length must not exceed 10000",
					"ctx": {
						"max_length": 10000,
						"actual_length": 10001
					}
				}
			]
		}
	},
	"tags": ["edge_cases", "string", "maxLength", "validation", "error"]
}
