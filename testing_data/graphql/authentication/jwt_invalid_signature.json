{
	"name": "jwt_invalid_signature",
	"description": "Query with JWT token having invalid signature",
	"operation_type": "query",
	"endpoint": "/graphql",
	"schema": "type Query {\n  currentUser: User\n}\n\ntype User {\n  id: String!\n  email: String!\n}",
	"request": {
		"method": "POST",
		"path": "/graphql",
		"query": "query {\n  currentUser {\n    id\n    email\n  }\n}",
		"headers": {
			"Content-Type": "application/json"
		},
		"body": {
			"query": "query {\n  currentUser {\n    id\n    email\n  }\n}"
		}
	},
	"expected_response": {
		"status_code": 401,
		"errors": [
			{
				"message": "Invalid token signature"
			}
		]
	},
	"resolvers": {
		"Query.currentUser": {
			"type": "error",
			"error_message": "Invalid token signature"
		}
	},
	"tags": ["authentication", "jwt", "invalid-signature", "error"]
}
