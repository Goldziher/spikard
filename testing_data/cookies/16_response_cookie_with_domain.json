{
  "name": "Response cookie with domain attribute",
  "description": "Tests setting cookie with specific domain",
  "source": {
    "framework": "starlette",
    "test_file": "tests/test_responses.py",
    "test_function": "test_set_cookie_with_domain"
  },
  "request": {
    "method": "POST",
    "path": "/cookies/set-with-domain",
    "body": {
      "value": "domain_test"
    }
  },
  "expected_response": {
    "status_code": 200,
    "body": {
      "message": "Cookie set with domain"
    },
    "cookies": {
      "session": {
        "value": "domain_test",
        "domain": ".example.com"
      }
    }
  },
  "notes": "Cookie with domain attribute restricts to .example.com"
}
