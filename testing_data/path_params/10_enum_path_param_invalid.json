{
  "name": "Enum path parameter - invalid value",
  "description": "Tests enum path parameter with invalid enum value returns 422",
  "category": "path_params",
  "source": {
    "framework": "fastapi",
    "test_file": "tests/test_tutorial/test_path_params/test_tutorial005.py",
    "test_function": "test_get_enums_invalid"
  },
  "handler": {
    "route": "/models/{model_name}",
    "method": "GET",
    "parameters": {
      "path": {
        "model_name": {
          "type": "string",
          "enum": [
            "alexnet",
            "resnet",
            "lenet"
          ]
        }
      }
    }
  },
  "request": {
    "method": "GET",
    "path": "/models/foo"
  },
  "expected_response": {
    "status_code": 422,
    "body": {
      "type": "https://spikard.dev/errors/validation-error",
      "title": "Request Validation Failed",
      "status": 422,
      "detail": "1 validation error in request",
      "errors": [
        {
          "type": "enum",
          "loc": [
            "path",
            "model_name"
          ],
          "msg": "Input should be 'alexnet', 'resnet' or 'lenet'",
          "input": "foo",
          "ctx": {
            "expected": "'alexnet', 'resnet' or 'lenet'"
          }
        }
      ]
    }
  },
  "notes": "Invalid enum value 'foo' not in allowed values (alexnet, lenet, resnet)"
}
