{
  "name": "Integer path parameter with combined lt and gt constraints - success",
  "description": "Tests integer path parameter with both lt and gt validation (range check)",
  "source": {
    "framework": "fastapi",
    "test_file": "tests/test_path.py",
    "test_function": "test_path_param_lt_gt_2"
  },
  "handler": {
    "route": "/path/param-lt-gt/{item_id}",
    "method": "GET",
    "parameters": {
      "path": {
        "item_id": {
          "type": "integer",
          "exclusiveMinimum": 1,
          "exclusiveMaximum": 3
        }
      }
    }
  },
  "request": {
    "method": "GET",
    "path": "/path/param-lt-gt/2"
  },
  "expected_response": {
    "status_code": 200,
    "body": {
      "item_id": 2
    }
  },
  "notes": "Path param with lt=3 and gt=1 constraints, value 2 is valid (1 < 2 < 3)"
}
