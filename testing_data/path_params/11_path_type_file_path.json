{
  "name": "Path type parameter - file path",
  "description": "Tests path type parameter that captures remaining path segments",
  "source": {
    "framework": "fastapi",
    "test_file": "tests/test_tutorial/test_path_params/test_tutorial004.py",
    "test_function": "test_file_path"
  },
  "handler": {
    "route": "/files/{file_path:path}",
    "method": "GET",
    "parameters": {
      "path": {
        "file_path": {
          "type": "string"
        }
      }
    }
  },
  "request": {
    "method": "GET",
    "path": "/files/home/johndoe/myfile.txt"
  },
  "expected_response": {
    "status_code": 200,
    "body": {
      "file_path": "home/johndoe/myfile.txt"
    }
  },
  "notes": "Path template: /files/{file_path:path}, captures all remaining path segments"
}
