{
  "name": "Boolean path parameter - numeric 1",
  "description": "Tests boolean path parameter with '1' converts to true",
  "source": {
    "framework": "fastapi",
    "test_file": "tests/test_path.py",
    "test_function": "test_path_bool_1"
  },
  "handler": {
    "route": "/path/bool/{item_id}",
    "method": "GET",
    "parameters": {
      "path": {
        "item_id": {
          "type": "boolean"
        }
      }
    }
  },
  "request": {
    "method": "GET",
    "path": "/path/bool/1"
  },
  "expected_response": {
    "status_code": 200,
    "body": {
      "item_id": true
    }
  },
  "notes": "Boolean conversion: '1' -> true"
}
