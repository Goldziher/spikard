{
	"name": "Integer path parameter with gt constraint - success",
	"description": "Tests integer path parameter with gt (greater than) validation succeeds",
	"source": {
		"framework": "fastapi",
		"test_file": "tests/test_path.py",
		"test_function": "test_path_param_gt_42"
	},
	"handler": {
		"route": "/path/param-gt/{item_id}",
		"method": "GET",
		"parameters": {
			"path": {
				"item_id": {
					"type": "integer",
					"exclusiveMinimum": 3
				}
			}
		}
	},
	"request": {
		"method": "GET",
		"path": "/path/param-gt/42"
	},
	"expected_response": {
		"status_code": 200,
		"body": {
			"item_id": 42
		}
	},
	"notes": "Path param with gt=3 constraint, value 42 is valid"
}
