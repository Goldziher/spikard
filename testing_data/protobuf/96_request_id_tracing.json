{
  "name": "Request ID for distributed tracing",
  "description": "Tests request ID header propagation for distributed tracing. Validates X-Request-ID generation and propagation.",
  "category": "auth_security",
  "protobuf": {
    "package": "example.v1",
    "messages": [
      {"name": "TracingRequest", "fields": [{"name": "operation", "type": "string", "number": 1, "label": "required"}]},
      {"name": "TracingResponse", "fields": [{"name": "request_id", "type": "string", "number": 1, "label": "optional"}, {"name": "trace_id", "type": "string", "number": 2, "label": "optional"}]}
    ],
    "services": [
      {
        "name": "TracingService",
        "methods": [
          {
            "name": "Trace",
            "input_type": "TracingRequest",
            "output_type": "TracingResponse"
          }
        ]
      }
    ]
  },
  "handler": {
    "service": "example.v1.TracingService",
    "method": "Trace",
    "middleware": {
      "request_id": {"enabled": true}
    }
  },
  "request": {
    "metadata": {
      "content-type": "application/grpc",
      "x-request-id": "req-12345-67890"
    },
    "message": {"operation": "trace_test"}
  },
  "expected_response": {
    "status_code": "OK",
    "message": {
      "request_id": "req-12345-67890"
    },
    "protobuf": {
      "metadata": {
        "x-request-id": "req-12345-67890"
      }
    }
  },
  "tags": ["tracing", "observability", "metadata"]
}
