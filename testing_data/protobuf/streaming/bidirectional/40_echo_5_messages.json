{
	"name": "Bidirectional streaming - echo 5 messages",
	"description": "Tests bidirectional streaming RPC where client sends 5 messages and expects them echoed back in the same order.",
	"category": "bidirectional_streaming",
	"protobuf": {
		"package": "example.v1",
		"messages": [
			{
				"name": "EchoMessage",
				"fields": [{ "name": "text", "type": "string", "number": 1, "label": "required" }]
			}
		],
		"services": [
			{
				"name": "EchoService",
				"methods": [
					{
						"name": "EchoBidi",
						"input_type": "EchoMessage",
						"output_type": "EchoMessage",
						"client_streaming": true,
						"server_streaming": true
					}
				]
			}
		]
	},
	"handler": {
		"service": "example.v1.EchoService",
		"method": "EchoBidi"
	},
	"request": {
		"metadata": {
			"content-type": "application/grpc"
		},
		"stream": [
			{ "text": "message_1" },
			{ "text": "message_2" },
			{ "text": "message_3" },
			{ "text": "message_4" },
			{ "text": "message_5" }
		]
	},
	"expected_response": {
		"status_code": "OK",
		"stream": [
			{ "text": "message_1" },
			{ "text": "message_2" },
			{ "text": "message_3" },
			{ "text": "message_4" },
			{ "text": "message_5" }
		]
	},
	"tags": ["bidirectional_streaming", "echo", "ordering"]
}
