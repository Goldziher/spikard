{
  "name": "PATCH - Partial update",
  "description": "Tests PATCH method for partial resource updates",
  "source": {
    "framework": "fastapi",
    "test_file": "tests/test_methods.py",
    "test_function": "test_patch_partial"
  },
  "request": {
    "method": "PATCH",
    "path": "/items/1",
    "headers": {
      "Content-Type": "application/json"
    },
    "body": {
      "price": 79.99
    }
  },
  "expected_response": {
    "status_code": 200,
    "body": {
      "id": 1,
      "name": "Existing Item",
      "price": 79.99,
      "in_stock": true
    }
  },
  "notes": "PATCH updates only specified fields, others remain unchanged"
}
