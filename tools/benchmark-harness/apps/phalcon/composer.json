{
	"name": "benchmark/phalcon",
	"description": "Phalcon PHP framework benchmark server for workload performance testing",
	"version": "1.0.0",
	"require": {
		"php": ">=8.2"
	},
	"require-dev": {
		"phpstan/phpstan": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"Benchmark\\Phalcon\\": "src/"
		}
	},
	"config": {
		"optimize-autoloader": true,
		"sort-packages": true,
		"allow-plugins": {}
	}
}
